<?xml version="1.0"?>
<transfers
        xmlns="spryker:transfer-01"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="RestUrlResolverAttributes">
        <property name="entityType" type="string"/>
        <property name="entityId" type="string"/>
    </transfer>

    <transfer name="UrlRedirectStorage">
        <property name="toUrl" type="string"/>
    </transfer>

    <transfer name="UrlStorage">
        <property name="fkResourceRedirect" type="int"/>
    </transfer>

    <transfer name="RestErrorMessage">
        <property name="status" type="int"/>
        <property name="code" type="string"/>
        <property name="detail" type="string"/>
    </transfer>

</transfers>
